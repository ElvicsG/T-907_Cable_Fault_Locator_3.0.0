<?xml version="1.0" encoding="utf-8"?>
<androidx.percentlayout.widget.PercentRelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:kosmos="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#00000000"
    android:gravity="center"
    android:orientation="vertical">

    <androidx.percentlayout.widget.PercentRelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/bg_dialog">

        <ImageView
            android:id="@+id/iv_close_auto"
            android:layout_width="@dimen/dp_20"
            android:layout_height="@dimen/dp_20"
            android:layout_alignParentEnd="true"
            android:layout_marginTop="@dimen/dp_5"
            android:layout_marginEnd="@dimen/dp_5"
            android:src="@drawable/bg_dialog_close_big"/>   <!--叉号位置//GC20220913-->

        <androidx.percentlayout.widget.PercentRelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_marginLeftPercent="9%"
            app:layout_marginRightPercent="9%"
            android:layout_centerVertical="true"
            android:gravity="center">

            <LinearLayout
                android:id="@+id/ll_note"
                android:layout_width="wrap_content"
                android:layout_height="@dimen/dp_32"
                android:layout_centerHorizontal="true"
                android:background="@drawable/bg_main_msg"
                android:gravity="center"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/tv_hv_gear"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:text="@string/hv_gear_note"
                    android:textColor="@color/colorPrimary"
                    android:textSize="@dimen/sp_10"
                    android:visibility="gone" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:text="   "
                    android:textColor="@color/colorPrimary"
                    android:textSize="@dimen/sp_10"
                    android:visibility="gone" />

                <TextView
                    android:id="@+id/tv_hv_working_mode"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:text="@string/hv_working_mode_note"
                    android:textColor="@color/colorPrimary"
                    android:textSize="@dimen/sp_10"
                    android:visibility="gone" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:text="   "
                    android:textColor="@color/colorPrimary"
                    android:textSize="@dimen/sp_10"
                    android:visibility="gone" />

                <TextView
                    android:id="@+id/tv_hv_capacitor"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:text="@string/hv_capacity_note"
                    android:textColor="@color/colorPrimary"
                    android:textSize="@dimen/sp_10"
                    android:visibility="gone" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:text="   "
                    android:textColor="@color/colorPrimary"
                    android:textSize="@dimen/sp_10"
                    android:visibility="gone" />

                <TextView
                    android:id="@+id/tv_hv_ignition_coil"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:text="@string/hv_ignition_coil_note"
                    android:textColor="@color/colorPrimary"
                    android:textSize="@dimen/sp_10"
                    android:visibility="gone" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:text="   "
                    android:textColor="@color/colorPrimary"
                    android:textSize="@dimen/sp_10"
                    android:visibility="gone" />

                <ImageView
                    android:id="@+id/iv_danger"
                    android:layout_width="@dimen/dp_32"
                    android:layout_height="wrap_content"
                    android:padding="@dimen/dp_5"
                    android:src="@drawable/ic_danger2"/>

                <TextView
                    android:id="@+id/tv_danger"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/danger"
                    android:textColor="@color/colorPrimary"
                    android:textSize="@dimen/sp_10" />  <!--危险提示添加//GC20220913-->

            </LinearLayout>

            <RelativeLayout
                android:id="@+id/rl_info"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/ll_note"
                android:layout_marginTop="@dimen/dp_15"
                android:focusable="true"
                android:focusableInTouchMode="true"
                android:gravity="center">   <!--高压操作模块位置下调//GC20220913-->
                <!--marginTop//GC20230227-->

                <RelativeLayout
                    android:id="@+id/rl_left"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"><!--//GC20221112-->

                    <!--接地报警-->
                    <TextView
                        android:id="@+id/tv_grounding_alarm"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/dp_20"
                        android:layout_marginTop="@dimen/dp_12"
                        android:text="@string/grounding_alarm"
                        android:textColor="@color/T_F2"
                        android:textSize="@dimen/sp_11"
                        android:visibility="gone" />

                    <ImageView
                        android:id="@+id/iv_warning"
                        android:layout_width="@dimen/dp_40"
                        android:layout_height="@dimen/dp_40"
                        android:layout_marginStart="@dimen/dp_26"
                        android:layout_toEndOf="@id/tv_grounding_alarm"
                        android:src="@drawable/light_gray"
                        android:visibility="gone" />    <!--去掉高压操作界面接地报警UI//GC20220913-->

                    <!--设定电压操作-->
                    <LinearLayout
                        android:id="@+id/ll_control_voltage"
                        android:layout_width="@dimen/dp_200"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/dp_12"
                        android:background="@drawable/shape_solide_back"
                        android:layout_below="@id/iv_warning"
                        android:gravity="center"
                        android:orientation="vertical"><!--高压设置界面调整//GC20221112--><!--layout_width//GC20230222-->

                        <!--设定电压-->
                        <LinearLayout
                            android:id="@+id/ll_value"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/dp_10"
                            android:gravity="center"
                            android:orientation="horizontal">

                            <ImageView
                                android:id="@+id/iv_lightning2"
                                android:layout_width="@dimen/dp_18"
                                android:layout_height="@dimen/dp_22"
                                android:src="@drawable/lightning_false"/><!--//GC20230228-->

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/set_voltage"
                                android:textColor="@color/colorPrimary"
                                android:textSize="@dimen/sp_15" />

                            <TextView
                                android:id="@+id/tv_value"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="0"
                                android:textColor="@color/colorPrimary"
                                android:textSize="@dimen/sp_15" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="kV"
                                android:textColor="@color/colorPrimary"
                                android:textSize="@dimen/sp_15" />
                        </LinearLayout>

                        <!--档位-->
                        <LinearLayout
                            android:id="@+id/ll_gear"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/dp_5"
                            android:gravity="center"
                            android:orientation="vertical"><!--//GC20221112-->

                            <RadioGroup
                                android:id="@+id/rg_gear"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:gravity="center"
                                android:orientation="horizontal">

                                <RadioButton
                                    android:id="@+id/rb_gear16"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:button="@drawable/rb_button_style"
                                    android:drawablePadding="@dimen/dp_15"
                                    android:paddingTop="@dimen/dp_5"
                                    android:paddingBottom="@dimen/dp_5"
                                    android:paddingStart="@dimen/dp_10"
                                    android:paddingEnd="@dimen/dp_10"
                                    android:text="@string/Gear1"
                                    android:textColor="@color/white"
                                    android:textSize="@dimen/sp_13" />

                                <RadioButton
                                    android:id="@+id/rb_gear32"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:button="@drawable/rb_button_style"
                                    android:drawablePadding="@dimen/dp_15"
                                    android:paddingTop="@dimen/dp_5"
                                    android:paddingBottom="@dimen/dp_5"
                                    android:paddingStart="@dimen/dp_10"
                                    android:paddingEnd="@dimen/dp_10"
                                    android:layout_marginStart="@dimen/dp_20"
                                    android:text="@string/Gear2"
                                    android:textColor="@color/white"
                                    android:textSize="@dimen/sp_13" />
                            </RadioGroup>
                        </LinearLayout>

                        <!--seekBar-->
                        <LinearLayout
                            android:id="@+id/ll_seekBar"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/dp_5"
                            android:layout_marginBottom="@dimen/dp_3"
                            android:gravity="center"
                            android:orientation="horizontal"><!--layout_marginBottom//GC20230222-->

                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:layout_marginEnd="@dimen/dp_5"
                                android:gravity="top"
                                android:orientation="vertical"><!--//GC20221206-->

                                <ImageView
                                    android:id="@+id/iv_minus"
                                    android:layout_width="@dimen/dp_22"
                                    android:layout_height="@dimen/dp_22"
                                    android:layout_marginTop="@dimen/dp_1"
                                    android:src="@drawable/bg_minus_selector"
                                    android:text="" />  <!--设定电压减号//GC20220927-->
                            </LinearLayout>

                            <net.kehui.www.t_907_origin_V3.ui.KBubbleSeekBar32
                                android:id="@+id/seekBar32"
                                android:layout_width="@dimen/dp_130"
                                android:layout_height="wrap_content"
                                kosmos:bsb_bubble_color="#24d1b4"
                                kosmos:bsb_bubble_text_color="#bc4306"
                                kosmos:bsb_bubble_text_size="28sp"
                                kosmos:bsb_colors="#ffEEEE00_#ffe1992e_#CD0000"
                                kosmos:bsb_marks="false"
                                kosmos:bsb_max="32"
                                kosmos:bsb_min="0"
                                kosmos:bsb_progress="0"
                                kosmos:bsb_second_track_color="#00F0FF"
                                kosmos:bsb_section_count="1"
                                kosmos:bsb_section_text_position="below_section_mark"
                                kosmos:bsb_show_progress_in_float="true"
                                kosmos:bsb_show_section_mark="false"
                                kosmos:bsb_show_section_text="true"
                                kosmos:bsb_show_thumb_text="true"
                                kosmos:bsb_thumb_color="#24d1b4"
                                kosmos:bsb_thumb_text_color="#bc4306"
                                kosmos:bsb_touch_to_seek="true"
                                kosmos:bsb_thumb_text_size="18sp"
                                kosmos:bsb_track_color="#606968"
                                kosmos:bsb_track_size="@dimen/dp_24"/>

                            <net.kehui.www.t_907_origin_V3.ui.KBubbleSeekBar16
                                android:id="@+id/seekBar16"
                                android:layout_width="@dimen/dp_130"
                                android:layout_height="wrap_content"
                                android:visibility="gone"
                                kosmos:bsb_bubble_color="#24d1b4"
                                kosmos:bsb_bubble_text_color="#bc4306"
                                kosmos:bsb_bubble_text_size="28sp"
                                kosmos:bsb_colors="#ffEEEE00_#ffe1992e_#CD0000"
                                kosmos:bsb_marks="false"
                                kosmos:bsb_max="16"
                                kosmos:bsb_min="0"
                                kosmos:bsb_progress="0"
                                kosmos:bsb_second_track_color="#00F0FF"
                                kosmos:bsb_section_count="4"
                                kosmos:bsb_section_text_position="below_section_mark"
                                kosmos:bsb_show_progress_in_float="true"
                                kosmos:bsb_show_section_mark="false"
                                kosmos:bsb_show_section_text="true"
                                kosmos:bsb_show_thumb_text="true"
                                kosmos:bsb_thumb_color="#24d1b4"
                                kosmos:bsb_thumb_text_color="#bc4306"
                                kosmos:bsb_thumb_text_size="18sp"
                                kosmos:bsb_touch_to_seek="true"
                                kosmos:bsb_track_color="#606968"
                                kosmos:bsb_track_size="@dimen/dp_24" />

                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:layout_marginStart="@dimen/dp_5"
                                android:gravity="top"
                                android:orientation="vertical"><!--//GC20221206-->

                                <ImageView
                                    android:id="@+id/iv_plus"
                                    android:layout_width="@dimen/dp_22"
                                    android:layout_height="@dimen/dp_22"
                                    android:layout_marginTop="@dimen/dp_1"
                                    android:src="@drawable/bg_plus_selector"
                                    android:text="" />  <!--设定电压加号//GC20220927-->

                                <TextView
                                    android:layout_width="@dimen/dp_22"
                                    android:layout_height="wrap_content"
                                    android:gravity="center"
                                    android:text="kV"
                                    android:textColor="@color/T_F2"
                                    android:textSize="@dimen/sp_11"
                                    kosmos:visibility="invisible" /><!--//GC20220618--><!--dp_22//GC20230222-->
                            </LinearLayout>
                        </LinearLayout>
                    </LinearLayout>

                    <!--电压确认-->
                    <LinearLayout
                        android:id="@+id/ll_confirm"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/ll_control_voltage"
                        android:layout_alignStart="@id/ll_control_voltage"
                        android:layout_alignEnd="@id/ll_control_voltage"
                        android:layout_marginTop="@dimen/dp_15"
                        android:gravity="center"
                        android:orientation="horizontal"><!--//GC20221112-->
                        <!--marginTop//GC20230227-->

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:paddingEnd="@dimen/dp_6"
                            android:paddingTop="@dimen/dp_2"
                            android:paddingBottom="@dimen/dp_2"
                            android:background="@drawable/shape_solide_blue"
                            android:gravity="center"
                            android:orientation="horizontal">

                            <ImageView
                                android:id="@+id/iv_lightning"
                                android:layout_width="@dimen/dp_18"
                                android:layout_height="@dimen/dp_22"
                                android:src="@drawable/lightning_false"/>  <!--闪电符号//GC20220711--><!--//GC20221112-->

                            <TextView
                                android:id="@+id/tv_confirm"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/hv_confirm"
                                android:textColor="@color/colorPrimary"
                                android:textSize="@dimen/sp_13" />
                        </LinearLayout>
                    </LinearLayout>

                    <!--击穿电压确认--><!--//GC20231009-->
                    <LinearLayout
                        android:id="@+id/ll_getBreakdownVoltage"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/ll_control_voltage"
                        android:layout_alignStart="@id/ll_control_voltage"
                        android:layout_alignEnd="@id/ll_control_voltage"
                        android:layout_marginTop="@dimen/dp_15"
                        android:gravity="center"
                        android:orientation="horizontal"
                        android:visibility="gone">

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:background="@drawable/shape_solide_blue"
                            android:gravity="center"
                            android:orientation="horizontal"
                            android:paddingStart="@dimen/dp_4"
                            android:paddingTop="@dimen/dp_2"
                            android:paddingEnd="@dimen/dp_4"
                            android:paddingBottom="@dimen/dp_2">

                            <TextView
                                android:id="@+id/tv_getBreakdownVoltage"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/hv_Breakdown_confirm"
                                android:textColor="@color/colorPrimary"
                                android:textSize="@dimen/sp_11" />
                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/ll_value_Breakdown"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/dp_5"
                            android:gravity="center"
                            android:orientation="horizontal">

                            <TextView
                                android:id="@+id/tv_note_Breakdown"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/Breakdown_voltage"
                                android:textColor="@color/colorPrimary"
                                android:textSize="@dimen/sp_11" />

                            <TextView
                                android:id="@+id/tv_value_Breakdown"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="0"
                                android:textColor="@color/colorPrimary"
                                android:textSize="@dimen/sp_11" />

                            <TextView
                                android:id="@+id/tv_kV"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="kV"
                                android:textColor="@color/colorPrimary"
                                android:textSize="@dimen/sp_11" />
                        </LinearLayout>
                    </LinearLayout>

                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_toEndOf="@+id/rl_left"
                    android:layout_marginStart="@dimen/dp_30"><!--//GC20221112-->

                    <!--工作方式新UI//GC20220913-->
                    <LinearLayout
                        android:id="@+id/ll_mode"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignStart="@id/ll_TIME"
                        android:layout_alignEnd="@id/ll_TIME"
                        android:gravity="center"
                        android:orientation="vertical"> <!--layout_alignStart/End//GC20230222-->

                        <RadioGroup
                            android:id="@+id/rg_gear_mode"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:orientation="horizontal">

                            <RadioButton
                                android:id="@+id/rb_gear_DC"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:button="@drawable/rb_button_style"
                                android:drawablePadding="@dimen/dp_15"
                                android:paddingStart="@dimen/dp_10"
                                android:paddingTop="@dimen/dp_15"
                                android:paddingEnd="@dimen/dp_10"
                                android:paddingBottom="@dimen/dp_15"
                                android:text="@string/DC"
                                android:textColor="@color/white"
                                android:textSize="@dimen/sp_13"
                                kosmos:visibility="gone" /><!--visibility="gone"//GC20221111-->

                            <RadioButton
                                android:id="@+id/rb_gear_PULSE"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:button="@drawable/rb_button_style"
                                android:drawablePadding="@dimen/dp_15"
                                android:paddingTop="@dimen/dp_15"
                                android:paddingBottom="@dimen/dp_15"
                                android:paddingStart="@dimen/dp_10"
                                android:paddingEnd="@dimen/dp_10"
                                android:text="@string/PULSE"
                                android:textColor="@color/white"
                                android:textSize="@dimen/sp_13" />

                            <RadioButton
                                android:id="@+id/rb_gear_CYCLIC"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:button="@drawable/rb_button_style"
                                android:drawablePadding="@dimen/dp_15"
                                android:paddingTop="@dimen/dp_15"
                                android:paddingBottom="@dimen/dp_15"
                                android:paddingStart="@dimen/dp_10"
                                android:paddingEnd="@dimen/dp_10"
                                android:layout_marginStart="@dimen/dp_20"
                                android:text="@string/CYCLIC"
                                android:textColor="@color/white"
                                android:textSize="@dimen/sp_13" /><!--layout_marginStart//GC20221111-->
                        </RadioGroup>
                    </LinearLayout>

                    <!--当前电压控制-->
                    <LinearLayout
                        android:id="@+id/ll_current_voltage"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignStart="@id/ll_TIME"
                        android:layout_alignEnd="@id/ll_TIME"
                        android:layout_below="@id/ll_mode"
                        android:gravity="center"
                        android:orientation="horizontal"><!--dp_190//GC20221112-->
                        <!--layout_alignStart/End//GC20230222-->

                        <!--当前电压显示-->
                        <LinearLayout
                            android:layout_width="@dimen/dp_85"
                            android:layout_height="@dimen/dp_70"
                            android:padding="@dimen/dp_3"
                            android:background="@drawable/shape_solide_back"
                            android:gravity="center"
                            android:orientation="vertical"><!--显示放大微调//GC20231027-->

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/dp_16"
                                android:gravity="center"><!--显示放大微调//GC20231027-->

                                <ImageView
                                    android:id="@+id/iv_voltage_height"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:src="@drawable/ic_vltage_height_0" />
                            </LinearLayout>

                            <!--当前电压--><!--显示放大微调//GC20231027-->
                            <LinearLayout
                                android:id="@+id/ll_HVINDICATOR"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/dp_3"
                                android:gravity="center"
                                android:orientation="horizontal">

                                <LinearLayout
                                    android:layout_width="@dimen/dp_48"
                                    android:layout_height="wrap_content"
                                    android:gravity="left"
                                    android:orientation="horizontal">

                                    <TextView
                                        android:id="@+id/tv_HVINDICATOR"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:text="0.00"
                                        android:textColor="@color/T_red"
                                        android:textSize="@dimen/sp_18" /><!--//GC20221206-->
                                </LinearLayout>

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="@dimen/dp_3"
                                    android:text="kV"
                                    android:textColor="@color/T_F2"
                                    android:textSize="@dimen/sp_18" /><!--//GC20221206-->
                            </LinearLayout>

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/HVINDICATOR"
                                android:textColor="@color/T_F2"
                                android:textSize="@dimen/sp_11" />
                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/ll_PULSE"
                            android:layout_width="@dimen/dp_75"
                            android:layout_height="@dimen/dp_75"
                            android:layout_marginStart="@dimen/dp_15"
                            android:orientation="vertical">

                            <ImageView
                                android:id="@+id/iv_HV_PULSE"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:src="@drawable/bg_pulse_selector"
                                android:text="" />
                        </LinearLayout>
                    </LinearLayout>

                    <!--放电周期-->
                    <LinearLayout
                        android:id="@+id/ll_TIME"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/ll_current_voltage"
                        android:gravity="center"
                        android:orientation="vertical"><!--dp_190//GC20221112-->

                        <LinearLayout
                            android:id="@+id/ll_seekBar_time"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/dp_10"
                            android:gravity="center"
                            android:orientation="horizontal"><!--marginTop//GC20230227-->

                            <LinearLayout
