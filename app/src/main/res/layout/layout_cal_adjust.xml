<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_margin="@dimen/dp_5"
    android:orientation="vertical">

    <ImageView
        android:id="@+id/tv_vop_j"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:layout_marginTop="@dimen/dp_4"
        android:src="@drawable/bg_jl_selector" />

    <ImageView
        android:id="@+id/tv_vop_d"
        android:layout_width="wrap_content"
        android:layout_marginTop="@dimen/dp_4"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:src="@drawable/bg_low_selector" />

    <ImageView
        android:id="@+id/tv_vop_g"
        android:layout_width="wrap_content"
        android:layout_marginTop="@dimen/dp_4"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:src="@drawable/bg_yjz_selector" />

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:orientation="vertical">
        <ImageView
            android:id="@+id/iv_cal_close1"
            android:layout_width="@dimen/dp_16"
            android:layout_height="@dimen/dp_16"
            android:layout_gravity="end"
            android:src="@drawable/bg_dialog_close" />
    </LinearLayout>

</LinearLayout>
